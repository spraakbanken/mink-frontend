    "js-yaml": "^4.1.0",
    "pinia": "^2.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "veaury": "^2.4.0",
    "vite": "^6.0.11",
    "vue": "^3.5.13",
    "vue-i18n": "11.0.1",
    "vue-matomo": "^4.2.0",
