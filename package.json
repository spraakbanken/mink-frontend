{
  "name": "mink-frontend",
  "version": "1.9.1",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "yarn run typecheck && vite build",
    "serve": "vite preview",
    "validate": "yarn run typecheck && yarn run lint && yarn run format",
    "fix": "yarn run lintfix && yarn run formatfix",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint",
    "lintfix": "eslint --fix",
    "format": "prettier . --check --ignore-path .gitignore",
    "formatfix": "prettier . --write --ignore-path .gitignore",
    "test": "vitest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/source-sans-3": "^5.1.1",
    "@formkit/vue": "^1.6.9",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@phosphor-icons/vue": "^2.2.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.5.0",
    "ajv": "^8.17.1",
    "axios": "^1.8.2",
    "datatransfer-files-promise": "^2.0.0",
    "es-toolkit": "^1.31.0",
    "eslint": "^9.19.0",
    "filesize": "^10.0.6",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.3.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.15",
    "vue": "^3.5.13",
    "vue-i18n": "11.1.2",
    "vue-matomo": "^4.2.0",
    "vue-router": "4.5.0",
    "vue-tsc": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.10",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^16.7.3",
    "prettier": "^3.4.2",
    "vite-plugin-checker": "^0.8.0",
    "vitest": "^3.0.5"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
