{
  "name": "mink-frontend",
  "version": "1.12.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "yarn run typecheck && vite build",
    "serve": "vite preview",
    "validate": "yarn run typecheck && yarn run lint && yarn run format",
    "fix": "yarn run lintfix && yarn run formatfix",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint",
    "lintfix": "eslint --fix",
    "format": "prettier . --check --ignore-path .gitignore",
    "formatfix": "prettier . --write --ignore-path .gitignore",
    "test": "vitest"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource-variable/source-sans-3": "^5.2.8",
    "@formkit/vue": "^1.6.9",
    "@highlightjs/vue-plugin": "^2.1.0",
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@phosphor-icons/vue": "^2.2.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.7.0",
    "ajv": "^8.17.1",
    "axios": "^1.11.0",
    "datatransfer-files-promise": "^2.0.0",
    "es-toolkit": "^1.39.10",
    "eslint": "^9.34.0",
    "filesize": "^11.0.2",
    "highlight.js": "^11.11.1",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "pinia": "^3.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vue": "^3.5.20",
    "vue-i18n": "11.1.11",
    "vue-matomo": "^4.2.0",
    "vue-router": "4.5.1",
    "vue-tsc": "^3.0.6"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.6.2",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "trailingComma": "all"
  }
}
